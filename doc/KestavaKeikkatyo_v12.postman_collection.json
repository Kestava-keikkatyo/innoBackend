				{
					"name": "Log in as jarmo-business",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const user = pm.response.json();\r",
									"\r",
									"pm.test(\"Has properties\", function () {\r",
									"    pm.expect(user).to.have.property('email');\r",
									"    pm.expect(user).to.have.property('token');\r",
									"    pm.expect(user).to.have.property('role');\r",
									"});\r",
									"\r",
									"if (pm.test(\"Has properties\")) {\r",
									"    pm.globals.set('innoToken', user.token);\r",
									"    pm.globals.set('role', user.role);\r",
									"}"
								],
								"type": "text/javascript"
							}
						}
